<UserControl x:Class="PZIS_4.TreeCircle"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PZIS_4"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="30">
    <Grid>
        <Ellipse Stroke="Black" Fill="#FFD2D2D2" Width="30" Height="30"/>
        <TextBox x:Name="textBox" Width="30" VerticalAlignment="Center" TextAlignment="Center" Background="{x:Null}" BorderBrush="{x:Null}" SelectionBrush="{x:Null}" MaxLength="3" MaxLines="1"  />
    </Grid>
</UserControl>
